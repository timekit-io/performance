{
    "name": "timekit-io/performance",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        ":vendor",
        ":package_name"
    ],
    "homepage": "https://github.com/timekit-io/performance",
    "license": "MIT",
    "authors": [
        {
            "name": "Visti Kl√∏ft",
            "email": "vk@timekit.io",
            "homepage": "https://timekit.io",
            "role": "Developer"
        },
        {
            "name": "Peter Suhm",
            "email": "peter@suhm.dk",
            "homepage": "https://petersuhm.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.0"
    },
    "require-dev": {
        "laravel/framework": "^5.3",
        "mockery/mockery": "^0.9.5",
        "nesbot/carbon": "^1.2",
        "phpunit/phpunit": "^5.6",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Timekit\\Performance\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Timekit\\Performance\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
